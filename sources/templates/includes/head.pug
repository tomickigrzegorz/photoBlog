mixin head(data, type)
  include googleAnalytics
  link(rel="dns-prefetch" href="//google-analytics.com")
  meta(charset="utf-8")
  meta(http-equiv="x-ua-compatible" content="ie=edge")
  meta(name="description", content=data.head.description)
  meta(name="viewport" content="width=device-width, initial-scale=1")
  meta(name="robots" content="index,follow")
  meta(name="google-site-verification" content="9P4FtusJunAS1pkZkQ_zCim0qapHt7_w9_Ly6WzrpkA") 
  //- title= data.head.title
  meta(name="theme-color" content="#ffffff")
  meta(name="msapplication-navbutton-color" content="#ffffff")
  meta(name="apple-mobile-web-app-status-bar-style" content="#ffffff")
  if type === 'article'
    script(defer, src="https://cdn.jsdelivr.net/gh/tomik23/zooom.js@master/docs/zooom.min.js") 
    script(defer, src="https://cdn.jsdelivr.net/gh/tomik23/reading-time@master/docs/readingTime.min.js") 
  title= data.head.title
  link(rel="manifest" href="/assets/manifest.json")
  link(rel="icon" type="image/x-icon" href="/favicon.ico")

  //-   include googleAdsense
